{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["enhancement", "feature"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["chore", "maintenance"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "## â¬†ï¸ Dependencies",
      "labels": ["dependencies"]
    },
    {
      "title": "## ğŸ—ï¸ Infrastructure",
      "labels": ["infrastructure", "ci", "cd"]
    }
  ],
  "ignore_labels": [
    "duplicate",
    "invalid",
    "wontfix"
  ],
  "sort": {
    "order": "DESC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{OWNER}}/${{REPOSITORY}}/compare/${{FROM_TAG}}...${{TO_TAG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}}) @${{AUTHOR}}",
  "empty_template": "No changes",
  "label_extractor": [
    {
      "pattern": "^fix\\s*:",
      "label": "fix"
    },
    {
      "pattern": "^feat\\s*:",
      "label": "feature"
    },
    {
      "pattern": "^docs\\s*:",
      "label": "documentation"
    },
    {
      "pattern": "^chore\\s*:",
      "label": "chore"
    }
  ],
  "duplicate_filter": {
    "pattern": "^.*",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "^fix\\s*:\\s*(.*)$",
      "target": "ğŸ› $1"
    },
    {
      "pattern": "^feat\\s*:\\s*(.*)$",
      "target": "âœ¨ $1"
    },
    {
      "pattern": "^docs\\s*:\\s*(.*)$",
      "target": "ğŸ“š $1"
    },
    {
      "pattern": "^chore\\s*:\\s*(.*)$",
      "target": "ğŸ”§ $1"
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver",
    "filter": {
      "pattern": "v(.+)",
      "flags": ""
    }
  },
  "base_branches": []
}