    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/x-data-grid": "^6.18.5",
    "@mui/x-date-pickers": "^6.18.5",
    "date-fns": "^3.0.6",
    "graphql": "^16.8.1",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "recharts": "^2.10.3",
    "uuid": "^9.0.1"
