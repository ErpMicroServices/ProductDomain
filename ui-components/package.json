    "prettier": "^3.1.1",
    "storybook": "^7.6.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErpMicroServices/ProductDomain.git"
  },
  "keywords": [
    "react",
    "components",
    "ui",
